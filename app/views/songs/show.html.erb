<a href="/songs">Dashboard</a>
<form action='/users' method='post'>
    <input type='hidden' name='authenticity_token' value='<%= form_authenticity_token %>'>
    <input type='hidden' name='_method' value='delete'>
    <input type='submit' value='Log Out'>
</form>

<h1><%= @song.artist%> - <%= @song.title %></h1>

<h3>Others who added <%= @song.artist%> - <%= @song.title %></h3>

<table>
  <tr>
    <th>Name</th>
    <th>Times Added</th>
  </tr>
  <tbody>
    <% @adds.each do |add| %>
    <tr>
      <td style="text-align:center"><a href="/users/<%=session[:user_id]%>"><%= add.user.first_name %> <%= add.user.last_name %></a> </td>
      <td style="text-align:center"><%= add.song.adds.count %></td>
    </tr>
    <% end %>
  </tbody>
</table>
